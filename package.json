{
  "name": "mifty-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "build:optimized": "node scripts/deploy.js",
    "test:a11y": "axe-core --dir build --include '**/*.html' --exclude '**/node_modules/**'",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "deploy:github": "npm run build:optimized && gh-pages -d build",
    "deploy:netlify": "npm run build:optimized && netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build:optimized && vercel --prod",
    "test:seo": "node scripts/seo-audit.js",
    "test:seo:dev": "npm run build && npm run test:seo",
    "ci:test": "npm run typecheck && npm run build && npm run test:a11y && npm run test:seo",
    "ci:deploy": "npm run ci:test && npm run deploy:github"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-lunr-search": "^3.6.0",
    "js-yaml": "^4.1.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vercel": "^48.10.2"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.0",
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/plugin-pwa": "^3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "gh-pages": "^6.3.0",
    "jsdom": "^23.2.0",
    "typescript": "~5.9.3",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "resolutions": {
    "@rollup/rollup-android-arm-eabi": "0.0.0"
  }
}
