        "selector": ".navbar__item.navbar__link--active",
        "global": true,
        "default_value": "Blog"
      },
      "lvl1": "article h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5",
      "lvl6": "article h6",
      "text": "article p, article li"
